{
  "name": "coach-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prisma:init && npm run next:dev",
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "prisma:init": "npm run prisma:migrate && npm run prisma:generate",
    "prisma:migrate": "npx prisma migrate up --experimental",
    "prisma:generate": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "2.5.1",
    "next": "9.3.5",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@prisma/cli": "2.5.1",
    "@types/node": "14.6.0",
    "@types/react": "16.9.46",
    "ts-node": "9.0.0",
    "typescript": "4.0.2"
  }
}
